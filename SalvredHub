local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Window = OrionLib:MakeWindow({Name = "SalvredHub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


local PlayerTab = Window:MakeTab({
 Name = "LocalPlayer",
 Icon = "rbxassetid://4483345998",
 PremiumOnly = false
})

local Section = PlayerTab:AddSection({
 Name = "LocalPlayer"
})

PlayerTab:AddSlider({
 Name = "Walkspeed",
 Min = 16,
 Max = 350,
 Default = 16,
 Color = Color3.fromRGB(255,255,255),
 Increment = 1,
 ValueName = "WS",
 Callback = function(Value)
  game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
 end    
})


local Universal = Window:MakeTab({
	Name = "Universal",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string> - The name of the tab.
Icon = <string> - The icon of the tab.
PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only.
]]


Universal:AddButton({
	Name = "FlyMobileV1",
	Callback = function()loadstring(game:HttpGet("https://pastebin.com/raw/XEhjj81x"))()
      		print("SexoKat")
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]


Universal:AddButton({
	Name = "CreatorCredits.",
	Callback = function()OrionLib:MakeNotification({
	Name = "Creator",
	Content = "Creator From Script: Salvred. Library Used: Orion Library",
	Image = "rbxassetid://4483345998",
	Time = 5
})

--[[
Title = <string> - The title of the notification.
Content = <string> - The content of the notification.
Image = <string> - The icon of the notification.
Time = <number> - The duration of the notfication.
]]
      		print("button pressed")
  	end    
})

