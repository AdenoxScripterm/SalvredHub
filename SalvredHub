    Default = "None",
    Options = {},  -- Las opciones se actualizar치n en tiempo real
    Callback = function(selectedName)
        selectedPlayer = Players:FindFirstChild(selectedName)
    end    
})

-- Actualizar la lista al iniciar
UpdateDropDownOptions(PlayerDropDown)

-- Conectar la actualizaci칩n de la lista cuando un jugador se une o se va
Players.PlayerAdded:Connect(function()
    UpdateDropDownOptions(PlayerDropDown)
end)

Players.PlayerRemoving:Connect(function()
    UpdateDropDownOptions(playerDropDown)
end)

