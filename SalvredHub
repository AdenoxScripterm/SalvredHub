-- Function to remove the 'BanBoxBrookhaven' folder
local function RemoveBanBrookhaven()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local BanBoxBrookhaven = ReplicatedStorage:FindFirstChild("BannedLots")

    if BanBoxBrookhaven and BanBoxBrookhaven:IsA("Folder") then
        -- Remove the 'BanBoxBrookhaven' folder
        BanBoxBrookhaven:ClearAllChildren()
        BanBoxBrookhaven:Destroy()

        -- Success notification
        OrionLib:MakeNotification({
            Name = "Success",
            Content = "Ban Was Removed :D lol",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    else
        -- Error notification
        OrionLib:MakeNotification({
            Name = "Error xd",
            Content = "Ban Remove is Already!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

-- Add button "Unban yourself from a house" to the "Unban" tab
Brookhaven:AddButton({
    Name = "Remove Ban",
    Callback = RemoveBanBrookhaven
})

-- Variables y servicios
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local selectedPlayerRoom = nil
local textboxValueRoom = nil

-- Funci칩n para actualizar la lista de jugadores en el dropdown
local function updatePlayerListRoom(dropdownRoom)
    local playerNamesRoom = {}

    for _, player in pairs(Players:GetPlayers()) do
        table.insert(playerNamesRoom, player.Name)
    end

    dropdownRoom:Refresh(playerNamesRoom, false)
end

-- A침adir el TextBox para ingresar el valor
Brookhaven:AddTextbox({
    Name = "House Number",
    Default = "",
    TextDisappear = false,
    Callback = function(value)
        textboxValueRoom = tonumber(value) -- Almacena el valor ingresado
    end
})

-- A침adir el Dropdown para seleccionar jugadores
local DropdownRoom = Brookhaven:AddDropdown({
    Name = "Select Player To Roomate",
    Default = "",
    Options = {}, -- Lista vac칤a que se actualizar치
    Callback = function(playerName)
        selectedPlayerRoom = Players:FindFirstChild(playerName) -- Almacena el jugador seleccionado
    end
})

-- Actualizar la lista de jugadores al iniciar
updatePlayerListRoom(DropdownRoom)

-- Actualizar la lista de jugadores cuando un jugador se une o sale
Players.PlayerAdded:Connect(function()
    updatePlayerListRoom(DropdownRoom)
end)

Players.PlayerRemoving:Connect(function()
    updatePlayerListRoom(DropdownRoom)
end)

-- A침adir el bot칩n para dar permisos
Brookhaven:AddButton({
    Name = "Give Roommate Permission To Selected Player",
    Callback = function()
        if selectedPlayerRoom and textboxValueRoom then
            -- Argumentos para el RemoteEvent
            local args = {
                [1] = "GivePermissionLoopToServer",
                [2] = selectedPlayerRoom, -- Jugador seleccionado en el dropdown
                [3] = textboxValueRoom -- Valor del textbox
            }

            -- Enviar evento al servidor
            ReplicatedStorage.RE:FindFirstChild("1Playe1rTrigge1rEven1t"):FireServer(unpack(args))
            OrionLib:MakeNotification({
                Name = "Success Roomate",
                Content = "Permission granted to " .. selectedPlayerRoom.Name,
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Please select a player and place the number of the house.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

local CarBrookhaven = Brookhaven:AddSection({
	Name = "Car Stuff"
})

-- Variables para almacenar Speed y Turbo
local Speed = 50 -- Velocidad por defecto
local Turbo = 50 -- Turbo por defecto

-- Funci칩n para cambiar la velocidad y el turbo del carro
local function ChangeCarSpeedAndTurbo(speedValue, turboValue)
    local player = game.Players.LocalPlayer
    local car = workspace.Vehicles:FindFirstChild(player.Name .. "Car")

    if car then
        local body = car:FindFirstChild("Body").VehicleSeat
        if body then
            -- Cambiar la velocidad m치xima
            body.TopSpeed.Value = speedValue
            -- Cambiar el valor del turbo
            body.Turbo.Value = turboValue
            
            OrionLib:MakeNotification({
                Name = "Applied",
                Content = "Speed: " .. tostring(speedValue) .. " | Turbo: " .. tostring(turboValue),
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "If I load your car well or the car went to waste xd",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    else
        OrionLib:MakeNotification({
            Name = "Error",
            Content = "Place A Car First :D",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

-- Crear TextBox para la velocidad (Speed)
Brookhaven:AddTextbox({
    Name = "Change Speed",
    Default = "50",
    TextDisappear = false,
    Callback = function(value)
        local newSpeed = tonumber(value)
        if newSpeed then
            Speed = newSpeed
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Place A Correct Number In The Speed...",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

-- Crear TextBox para el turbo (Turbo)
Brookhaven:AddTextbox({
    Name = "Change Turbo",
    Default = "50",
    TextDisappear = false,
    Callback = function(value)
        local newTurbo = tonumber(value)
        if newTurbo then
            Turbo = newTurbo
        else
            OrionLib:MakeNotification({
                Name = "Error",
                Content = "Place A Correct Number In The Turbo...",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

-- Bot칩n para aplicar los cambios de Speed y Turbo
Brookhaven:AddButton({
    Name = "Apply Speed And Turbo",
    Callback = function()
        ChangeCarSpeedAndTurbo(Speed, Turbo)
    end
})

local PlayerStuffBrookhaven = Brookhaven:AddSection({
	Name = "You"
})

local ToggleColorsBrookhavenRPNameColor = false
local ColorChangeLoopBrookhavenRPNameColor = nil

local function ChangeColorBrookhavenRPNameColor()
    while ToggleColorsBrookhavenRPNameColor do
        local ColorsBrookhavenRPNameColor = {
            Color3.fromRGB(255, 0, 0),   -- Rojo
            Color3.fromRGB(255, 128, 0), -- Naranja
            Color3.fromRGB(255, 255, 0), -- Amarillo
            Color3.fromRGB(128, 255, 0), -- Verde claro
            Color3.fromRGB(0, 255, 0),   -- Verde
            Color3.fromRGB(0, 255, 128), -- Verde aqua
            Color3.fromRGB(0, 255, 255), -- Cian
            Color3.fromRGB(0, 128, 255), -- Azul claro
            Color3.fromRGB(0, 0, 255),   -- Azul
            Color3.fromRGB(128, 0, 255), -- P칰rpura
            Color3.fromRGB(255, 0, 255), -- Magenta
            Color3.fromRGB(255, 0, 128)  -- Rosa
        }
        for _, ColorBrookhavenRPNameColor in pairs(ColorsBrookhavenRPNameColor) do
            if not ToggleColorsBrookhavenRPNameColor then return end
            local ArgsBrookhavenRPNameColor = {
                [1] = "PickingRPNameColor",
                [2] = ColorBrookhavenRPNameColor
            }
            game:GetService("ReplicatedStorage").RE["1RPNam1eColo1r"]:FireServer(unpack(ArgsBrookhavenRPNameColor))
            wait(1) -- Espera 1 segundo antes de cambiar al siguiente color
        end
    end
end

-- A침adir el toggle a la pesta침a
Brookhaven:AddToggle({
    Name = "Rainbow Color Name",
    Default = false,
    Callback = function(ValueBrookhavenRPNameColor)
        ToggleColorsBrookhavenRPNameColor = ValueBrookhavenRPNameColor
        if ToggleColorsBrookhavenRPNameColor then
            ColorChangeLoopBrookhavenRPNameColor = coroutine.wrap(ChangeColorBrookhavenRPNameColor)
            ColorChangeLoopBrookhavenRPNameColor()
        end
    end
})

local ToggleColorsBrookhavenRPBioColor = false
local ColorChangeLoopBrookhavenRPBioColor = nil

