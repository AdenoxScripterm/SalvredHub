-- Fun칞칚o para teleportar e matar o jogador
local function TeleportAndKillPlayerBrookhaven(TargetPlayerBrookhaven)
    if not TargetPlayerBrookhaven then
        -- Notifica칞칚o se nenhum jogador for selecionado
        OrionLib:MakeNotification({
            Name = "Error",
            Content = "Select A Player",
            Time = 5
        })
        return
    end

    local MyHRPBrookhaven = game.Players.LocalPlayer.Character.HumanoidRootPart
    local TargetHRPBrookhaven = TargetPlayerBrookhaven.Character:WaitForChild("HumanoidRootPart")
    
    -- Armazena a posi칞칚o original
    local OriginalPositionBrookhaven = MyHRPBrookhaven.CFrame
    
    -- Teleporta para a posi칞칚o do alvo
    MyHRPBrookhaven.CFrame = TargetHRPBrookhaven.CFrame
    wait(1)
    
    local ConnectionBrookhaven
    ConnectionBrookhaven = game:GetService("RunService").Stepped:Connect(function()
        MyHRPBrookhaven.CFrame = TargetHRPBrookhaven.CFrame
        if TargetPlayerBrookhaven.Character and TargetPlayerBrookhaven.Character:FindFirstChild("Humanoid") and TargetPlayerBrookhaven.Character.Humanoid.Sit then
            -- Retorna  posi칞칚o original
            MyHRPBrookhaven.CFrame = OriginalPositionBrookhaven
            ConnectionBrookhaven:Disconnect()
            wait("0.9")
            local Player = game.Players.LocalPlayer
local BackPack = Player:WaitForChild("Backpack")

local Couch = Player.Character:FindFirstChild("Couch")
if Couch then
    Couch.Parent = BackPack
    print("Success Teleport To You The Player Selected.")
else
    print("Mmmm")
end
        end
    end)
end

-- Adiciona o bot칚o para matar o jogador
Brookhaven:AddButton({
    Name = "Bring Selected Player",
    Callback = function()
         local args = {
    [1] = "PlayerWantsToDeleteTool",
    [2] = "Couch"
}

game:GetService("ReplicatedStorage").RE:FindFirstChild("1Clea1rTool1s"):FireServer(unpack(args))

wait("0.5")

local args = {
    [1] = "PickingTools",
    [2] = "Couch"
}

game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))
        wait("0.5")
        local Player = game.Players.LocalPlayer
local BackPack = Player:WaitForChild("Backpack")

local Couch = BackPack:FindFirstChild("Couch")
if Couch then
    Player.Character:WaitForChild("Humanoid"):EquipTool(Couch)
else
    print("Couch Not Founded")
end
        TeleportAndKillPlayerBrookhaven(SelectedPlayerBrookhaven)
    end    
})

